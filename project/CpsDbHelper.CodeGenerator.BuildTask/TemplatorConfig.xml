    <CustomOptions Key="Filters" Value=".xml,.csv,.txt" Category="SyntaxBuildTask"></CustomOptions>
    <CustomOptions Key="Path" Value="Templates" Category="SyntaxBuildTask"></CustomOptions>
    <CustomOptions Key="Depth" Value="3" Category="SyntaxBuildTask"></CustomOptions>
