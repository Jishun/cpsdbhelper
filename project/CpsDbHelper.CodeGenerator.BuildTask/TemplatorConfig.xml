<TemplatorConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <XmlTemplatorAttributeName>bindings</XmlTemplatorAttributeName>
    <ReservedKeywordParent>$$P$$</ReservedKeywordParent>
    <ReservedKeywordIndex>$Index</ReservedKeywordIndex>
    <ReservedKeyword0Index>$0Index</ReservedKeyword0Index>
    <Begin>{{</Begin>
    <End>}}</End>
    <Delimiter>,</Delimiter>
    <ParamBegin>(</ParamBegin>
    <ParamEnd>)</ParamEnd>
    <CategorizedNameBegin>(</CategorizedNameBegin>
    <CategorizedNameEnd>)</CategorizedNameEnd>
    <KeywordsBegin>[</KeywordsBegin>
    <KeywordsEnd>]</KeywordsEnd>
    <SyntaxErrorCategoryNotFound>SyntaxError: No category defined in Holder, add use Category(Name) to define name or set CategoryOptional=true in config</SyntaxErrorCategoryNotFound>
    <SyntaxErrorNestedHolders>SyntaxError: Nested Holders Found: '{0}', Prevent nesting or set AllowNested=true</SyntaxErrorNestedHolders>
    <SyntaxErrorOverLappedHolder>SyntaxError: Overlapped TextHolders</SyntaxErrorOverLappedHolder>
    <SyntaxErrorInvalidCategory>SyntaxError: Invalid category : '{0}'</SyntaxErrorInvalidCategory>
    <SyntaxErrorUnmatchedBeginTag>SyntaxError: Reached document end before finding the closing tag</SyntaxErrorUnmatchedBeginTag>
    <SyntaxErrorUnmatchedKeywordsBeginTag>SyntaxError: Reached document end before finding keywords the closing tag</SyntaxErrorUnmatchedKeywordsBeginTag>
    <SyntaxErrorUnmatchedBeginNameTag>SyntaxError: Reached document end before finding the name closing tag</SyntaxErrorUnmatchedBeginNameTag>
    <SyntaxErrorHolderNameNotFound>SyntaxError: TextHolder's Name not defined</SyntaxErrorHolderNameNotFound>
    <SyntaxErrorUnexpectedString>SyntaxError: Unexpcted string: '{0}'</SyntaxErrorUnexpectedString>
    <SyntaxErrorUnexpectedKeywordParam>SyntaxError: Unexpcted param for keyword: '{0}'</SyntaxErrorUnexpectedKeywordParam>
    <SyntaxErrorUnexpectedKeyword>SyntaxError: Unexpcted keyword: '{0}'</SyntaxErrorUnexpectedKeyword>
